{
  "name": "api",
  "version": "1.0.0",
  "description": "Azure Functions for TheHealthFoodCompanyApp",
  "scripts": {
    "build": "tsc && npm run copy-function-json",
    "copy-function-json": "powershell \"Get-ChildItem -Path . -Filter function.json -Recurse | ForEach-Object { Copy-Item $_.FullName -Destination (Join-Path 'dist' $_.Directory.Name) -Force }\"",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "start": "func start",
    "test": "echo \"No tests yet\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/functions": "^4.5.0",
    "jsonwebtoken": "^9.0.2",
    "@azure/data-tables": "^13.2.2"
  },
  "devDependencies": {
    "typescript": "^5.5.3",
    "@types/jsonwebtoken": "^9.0.6"
  }
}
